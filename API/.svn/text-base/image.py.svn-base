# -*- coding: utf-8 -*- 

class Image:
    image = None #이미지 URL
    thumbnail = None #썸네일 URL
    width = None #이미지의 가로 크기
    height = None #이미지의 세로 크기
    tag = None #이미지의 부가태그 


    def __repr__(self):
        toStr = "image:"+self.image+"\n"
        toStr += "thumbnail:"+self.thumbnail+"\n"
        toStr += "width:"+self.width+"\n"
        toStr += "height:"+self.height+"\n"
        toStr += "tag :"+self.tag
        return toStr


